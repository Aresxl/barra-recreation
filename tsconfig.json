{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/ui/*": ["src/components/ui/*"],
      "@/images*/": ["src/assets/images/*"],
      "@/sections/*": ["src/components/sections/*"]
    }
  }
}
